@{
    Layout = "master.cshtml";
    ViewBag.Title = "Login";
}

<script>
  const returnUrl = '@(Request.Query["returnUrl"])';
  const errorMsg = '@ViewBag.AuthenticationError';
  const loginView = new App.Views.Account.LoginView(returnUrl, errorMsg);

  m.mount(document.body, loginView);
</script>
