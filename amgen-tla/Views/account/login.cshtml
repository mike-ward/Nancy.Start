@{
    Layout = "master.cshtml";
    ViewBag.Title = "Login";
}

<script>
  const authenticationUrl = '@ViewBag.AuthenticationUrl';
  const returnUrl = '@(Request.Query["returnUrl"])';
  const errorMsg = '@ViewBag.AuthenticationError';

  mountView(App.Views.Account.loginView(authenticationUrl, returnUrl, errorMsg));
</script>
