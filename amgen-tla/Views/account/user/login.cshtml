@using TLA.Models.Authentication
@{
    Layout = "master.cshtml";
    ViewBag.Title = "Login";
}

<script>
  const returnUrl = '@(Request.Query["returnUrl"])';
  const errorMsg = '@ViewBag.AuthenticationError';

  mountView(App.Views.Account.User.loginView('@AuthenticationRedirectUrl.Url', returnUrl, errorMsg));
</script>
