@using App.Models.Authentication
@{
    Layout = "master.cshtml";
    ViewBag.Title = "Login";
}

<script>
  const authenticationUrl = '@AuthenticationRedirectUrl.Url';
  const returnUrl = '@(Request.Query["returnUrl"])';
  const errorMsg = '@ViewBag.AuthenticationError';

  mountView(App.Views.Account.User.loginView(authenticationUrl, returnUrl, errorMsg));
</script>
